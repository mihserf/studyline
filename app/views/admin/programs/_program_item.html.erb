<li style="list-style:none; padding-left:<%= it.level*20 %>px;"><%= link_to it.name, l_program_path(lang_id(@lang),it.permalink) %>
  <div class='admin_edit_actions' style="font-size:10pt;  padding-bottom:7px;">
    <%@locales.each do |loc|%>
        <%=loc.short%><%= link_to image_tag("/images/edit_ico.gif",:alt=>"Редактировать"), edit_admin_l_program_path(loc.short,it) %>
    <%end%>
     | <%=link_to image_tag("/images/delete_ico.gif",:alt=>"Удалить"), admin_program_path(it), :method => :delete, :confirm => 'подтвердите удаление'%>
  </div>
</li> 