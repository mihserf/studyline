<% content_for :meta do %>
    <% meta = {:description => (obj.metatag_description if obj && !(obj.is_a?(String))) || 'StudyLine', :keywords => (obj.metatag_keywords if obj && !(obj.is_a?(String))) || 'StudyLine'} %>
    <%= meta_tags(meta) %>
    <% html_title_str = obj.is_a?(String) ? obj : (obj.metatag_title if obj && obj.metatag_title) %>
    <% title = title.nil? ? "" : title %>
    <%= html_title("#{title}
      #{html_title_str if html_title_str}", " ") %>
<% end %>